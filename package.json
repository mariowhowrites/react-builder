{
  "name": "react-builder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.4.10",
    "lodash": "^4.17.13",
    "postcss-cli": "^6.1.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8",
    "tailwindcss": "^0.7.4"
  },
  "scripts": {
    "build:css": "postcss src/App.css -o src/index.css",
    "watch:css": "postcss src/App.css -o src/index.css -w",
    "start": "npm run watch:css & react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.16.4"
  }
}
